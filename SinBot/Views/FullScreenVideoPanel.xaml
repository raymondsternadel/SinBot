<Window x:Class="SinBot.Views.FullScreenVideoPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SinBot.Views"
        mc:Ignorable="d"
        Title="FullScreenVideoPanel" Height="1100" Width="1950" WindowStyle="None" Background="Transparent" WindowStartupLocation="CenterScreen"
        Loaded="Window_Loaded">
    <MediaElement x:Name="FullScreenMediaElement" LoadedBehavior="Manual" Volume="100" MediaEnded="FullScreenMediaElement_MediaEnded"/>
</Window>
