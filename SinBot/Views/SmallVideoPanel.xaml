<Window x:Class="SinBot.Views.SmallVideoPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SinBot.Views"
        mc:Ignorable="d"
        Title="SmallVideoPanel" Height="240" Width="426" WindowStyle="None" Background="Transparent" Loaded="Window_Loaded"
        Topmost="True" ResizeMode="NoResize">
    <StackPanel Background="Transparent">
        <MediaElement LoadedBehavior="Play" Stretch="Fill" MediaEnded="myMediaElement_MediaEnded" x:Name="myMediaElement" Volume="100"/>
    </StackPanel>
</Window>